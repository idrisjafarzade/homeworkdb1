@model SliderImgIcons
@{
    ViewData["Title"] = "Create";
  
}

<div class="container">
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create Slider Image</h4>
             <form class="forms-sample" method="post" enctype="multipart/form-data">
              <div class="form-group">
                    <label asp-for="Photos">Photos</label>
                    <input asp-for="Photos" class="form-control" placeholder="Photos" accept="image/*">
                    <span class="text-danger" asp-validation-for="Photos"></span>
              </div>
                    <button type="submit" class="btn btn-primary mr-2 ">Submit</button>
                 <a class="btn btn-light"  asp-action="Index">Cancel</a>
            </form>
        </div>
        </div>
    </div>
</div>
</div>

@section Scripts{
    <partial name="_ValidationPartial"/>
}

